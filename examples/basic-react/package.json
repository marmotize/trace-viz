{
  "name": "trace-viz-example",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build:storybook": "storybook build",
    "dev": "vite",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "test:storybook": "concurrently -k -s first -n 'SB,TEST' -c 'magenta,blue' 'pnpm storybook --no-open --quiet' 'wait-on http://127.0.0.1:6006 && test-storybook --ci'",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@trace-viz/core": "workspace:*",
    "@trace-viz/react": "workspace:*",
    "@trace-viz/version-detector-jsonata": "workspace:*",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-vite": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/test-runner": "^0.23.0",
    "@types/node": "^24.9.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.4",
    "concurrently": "^9.2.1",
    "storybook": "^8.6.14",
    "typescript": "^5.3.3",
    "vite": "^7.1.11",
    "wait-on": "^9.0.1"
  }
}
