    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@trace-viz/core": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^19.2.0",
    "react": "^19.2.0",
    "tsdown": "^0.15.6",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "peerDependencies": {
    "react": "^18 || ^19"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    }
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
