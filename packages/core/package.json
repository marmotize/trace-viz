{
  "name": "@trace-viz/core",
  "version": "0.1.0",
  "description": "Core trace visualization orchestration library",
  "keywords": [
    "trace",
    "visualization"
  ],
  "homepage": "https://github.com/marmotize/trace-viz/tree/main/packages/core#readme",
  "bugs": {
    "url": "https://github.com/marmotize/trace-viz/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marmotize/trace-viz.git",
    "directory": "packages/core"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
